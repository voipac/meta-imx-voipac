+int detect_board(void)
+{
+	int ret = (gpio_get_value(DDR_DET_0) | gpio_get_value(DDR_DET_1) << 1 |
+		gpio_get_value(DDR_DET_2) << 2 | gpio_get_value(DDR_DET_3) << 3) & 0xF;
+
+	printf("%d-%d-%d-%d\n", gpio_get_value(DDR_DET_3), gpio_get_value(DDR_DET_2), gpio_get_value(DDR_DET_1), gpio_get_value(DDR_DET_0));
+
+	printf("Detected board nr:%d\n", ret);
+	return ret;
+}
