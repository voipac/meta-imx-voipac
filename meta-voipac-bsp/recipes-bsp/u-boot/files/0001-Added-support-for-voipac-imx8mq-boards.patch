+static void spl_dram_init(void)
+{
+	// setup mux for module detection
+	setup_iomux_ver_det();
+
+	int board = detect_board();
+	
+	printf("RAM size:%dG\n", desc[board].ram_size);
+	
+	switch(desc[board].ram_size) {
+	case 1:
+		ddr_init(&dram_timing_voipac_1g);
+		writel(0x1, MCU_BOOTROM_BASE_ADDR);
+		break;
+	
+	case 2:
+		ddr_init(&dram_timing_voipac_2g);
+		writel(0x2, MCU_BOOTROM_BASE_ADDR);
+		break;		
+	case 4:
+		ddr_init(&dram_timing_voipac_4g);
+		writel(0x4, MCU_BOOTROM_BASE_ADDR);
+		break;	
+	default:
+		printf("Unknown ram config\n");
+		ddr_init(&dram_timing_voipac_1g);
+		writel(0x1, MCU_BOOTROM_BASE_ADDR);
+	}
+}
