From: Marek Belisko <marek.belisko@open-nandra.com>
Date: Fri, 24 May 2024 16:48:31 +0200
Subject: [PATCH] Drop building some dtbs + small drops

Signed-off-by: Marek Belisko <marek.belisko@open-nandra.com>
---
 arch/arm/dts/Makefile                                | 7 -------
 arch/arm/dts/imx93-11x11-evk-pmic-pf0900-u-boot.dtsi | 2 --
 arch/arm/dts/imx93-11x11-evk-pmic-pf0900.dts         | 1 -
 arch/arm/dts/imx93-11x11-evk.dts                     | 1 -
 4 files changed, 11 deletions(-)

diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index ceec5c074a..43af018669 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -1088,13 +1088,6 @@ dtb-$(CONFIG_ARCH_IMX9) += \
 	imx95-19x19-evk.dtb \
 	imx95-19x19-titan.dtb \
 	imx93-11x11-evk.dtb \
-	imx93-11x11-evk-pmic-pf0900.dtb \
-	imx93-14x14-evk.dtb \
-	imx93-9x9-qsb.dtb \
-	imx93-9x9-qsb-ontat-wvga-panel.dtb \
-	imx91p-11x11-evk.dtb \
-	imx91p-9x9-qsb-ontat-wvga-panel.dtb \
-	imx91p-9x9-qsb-spinand.dtb
 
 dtb-$(CONFIG_ARCH_IMXRT) += imxrt1050-evk.dtb \
 	imxrt1020-evk.dtb \
diff --git a/arch/arm/dts/imx93-11x11-evk-pmic-pf0900-u-boot.dtsi b/arch/arm/dts/imx93-11x11-evk-pmic-pf0900-u-boot.dtsi
index 49dec5f713..0ad9d7e188 100644
--- a/arch/arm/dts/imx93-11x11-evk-pmic-pf0900-u-boot.dtsi
+++ b/arch/arm/dts/imx93-11x11-evk-pmic-pf0900-u-boot.dtsi
@@ -146,13 +146,11 @@
 };
 
 &fec {
-	phy-reset-gpios = <&pcal6524 16 GPIO_ACTIVE_LOW>;
 	phy-reset-duration = <15>;
 	phy-reset-post-delay = <100>;
 };
 
 &ethphy1 {
-	reset-gpios = <&pcal6524 15 GPIO_ACTIVE_LOW>;
 	reset-assert-us = <15000>;
 	reset-deassert-us = <100000>;
 };
diff --git a/arch/arm/dts/imx93-11x11-evk-pmic-pf0900.dts b/arch/arm/dts/imx93-11x11-evk-pmic-pf0900.dts
index 5fdd52e81e..b34a79c75d 100644
--- a/arch/arm/dts/imx93-11x11-evk-pmic-pf0900.dts
+++ b/arch/arm/dts/imx93-11x11-evk-pmic-pf0900.dts
@@ -11,7 +11,6 @@
 	pmic1: pmic@8 {
 		compatible = "nxp,pf0900";
 		reg = <0x08>;
-		interrupt-parent = <&pcal6524>;
 		interrupts = <11 IRQ_TYPE_EDGE_FALLING>;
 		status = "okay";
 
diff --git a/arch/arm/dts/imx93-11x11-evk.dts b/arch/arm/dts/imx93-11x11-evk.dts
index 559e806a99..a088239120 100644
--- a/arch/arm/dts/imx93-11x11-evk.dts
+++ b/arch/arm/dts/imx93-11x11-evk.dts
@@ -12,7 +12,6 @@
 	compatible = "fsl,imx93-11x11-evk", "fsl,imx93";
 
 	aliases {
-		rtc0 = &pcf2131;
 	 };
 
 	chosen {
