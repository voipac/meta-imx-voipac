From 859f069d2bc9daafedceb2a7f8d4176ff946f393 Mon Sep 17 00:00:00 2001
From: Marek Belisko <marek.belisko@gmail.com>
Date: Thu, 16 May 2024 11:18:31 +0200
Subject: [PATCH] Drop eno nodes

Signed-off-by: Marek Belisko <marek.belisko@gmail.com>
---
 .../boot/dts/freescale/imx93-11x11-evk-rpmsg.dts |  3 ---
 .../arm64/boot/dts/freescale/imx93-11x11-evk.dts | 16 ++--------------
 2 files changed, 2 insertions(+), 17 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/imx93-11x11-evk-rpmsg.dts b/arch/arm64/boot/dts/freescale/imx93-11x11-evk-rpmsg.dts
index 3627491cd1ea..d1d1ab2ffd14 100644
--- a/arch/arm64/boot/dts/freescale/imx93-11x11-evk-rpmsg.dts
+++ b/arch/arm64/boot/dts/freescale/imx93-11x11-evk-rpmsg.dts
@@ -6,11 +6,8 @@
 #include "imx93-11x11-evk.dts"
 
 /delete-node/ &lpi2c1;
-/delete-node/ &ethosu_mem;
 
 / {
-	/delete-node/ ethosu;
-
 	aliases {
 		i2c0 = &i2c_rpbus_1;
 	};
diff --git a/arch/arm64/boot/dts/freescale/imx93-11x11-evk.dts b/arch/arm64/boot/dts/freescale/imx93-11x11-evk.dts
index 09c4f189b543..a8c8f15c014f 100644
--- a/arch/arm64/boot/dts/freescale/imx93-11x11-evk.dts
+++ b/arch/arm64/boot/dts/freescale/imx93-11x11-evk.dts
@@ -29,16 +29,11 @@ linux,cma {
 			compatible = "shared-dma-pool";
 			reusable;
 			alloc-ranges = <0 0x80000000 0 0x40000000>;
-			size = <0 0x10000000>;
+			size = <0 0x3200000>;
 			linux,cma-default;
 		};
 
-		ethosu_mem: ethosu_region@C0000000 {
-			compatible = "shared-dma-pool";
-			reusable;
-			reg = <0x0 0xC0000000 0x0 0x10000000>;
-		};
-
+		
 		vdev0vring0: vdev0vring0@a4000000 {
 			reg = <0 0xa4000000 0 0x8000>;
 			no-map;
@@ -77,13 +72,6 @@ ele_reserved: ele-reserved@a4120000 {
 		};
 	};
 
-	ethosu {
-		compatible = "arm,ethosu";
-		fsl,cm33-proc = <&cm33>;
-		memory-region = <&ethosu_mem>;
-		power-domains = <&mlmix>;
-	};
-
 	reg_can2_stby: regulator-can2-stby {
 		compatible = "regulator-fixed";
 		regulator-name = "can2-stby";
-- 
2.25.1

